---
layout: null        # hiçbir Jekyll şablonu uygulanmasın
permalink: /feedR.xml
---


<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
  <channel>
    <title>{{ site.title }} – R Posts</title>
    <link>{{ site.url }}{{ site.baseurl }}/blog-posts-for-r/</link>
    <description>{{ site.description | xml_escape }}</description>

    {% assign rPosts = site.posts | where_exp:"p","p.tags contains 'r' or p.categories contains 'r'" %}
    {% for post in rPosts %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <link>{{ site.url }}{{ post.url }}</link>
        <guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
        <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
        <description>{{ post.excerpt | strip_html | strip_newlines | xml_escape }}</description>
      </item>
    {% endfor %}
  </channel>
</rss>
